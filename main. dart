import 'package:flutter/material.dart';
import 'package:firebase_core/firebase_core.dart'; // ðŸ‘ˆ Required for Firebase
import 'question1.dart'; // ðŸ‘ˆ Your first screen

void main() async {
  WidgetsFlutterBinding.ensureInitialized(); // ðŸ‘ˆ Required before Firebase.init
  await Firebase.initializeApp(); // ðŸ‘ˆ Initializes Firebase

  runApp(MaterialApp(
    home: QuestionOneScreen(), // ðŸ‘ˆ Starts your app from this screen
    debugShowCheckedModeBanner: false,
  ));
}







